unit UNodeTest;

interface

uses
  DUnitX.TestFramework, UNodes;

type
  [TestFixture]
  TStartTestObject = class
  strict private
    aStartNode : TStart;
  public
    [Setup]
    procedure Setup;
    [TearDown]
    procedure TearDown;
    [TestCase('Test setze Start-ID','5')]
    procedure setID(newID : Integer);
  end;

implementation

procedure TStartTestObject.Setup;
begin
  aStartNode := TStart.Create(1);
end;

procedure TStartTestObject.TearDown;
begin
end;

procedure TStartTestObject.setID(newID : Integer);
var
  R : Integer;
begin
  aStartNode.setID(newID);
  R := aStartNode.getID;
  Assert.AreEqual(R,newID);
end;


initialization
  TDUnitX.RegisterTestFixture(TStartTestObject);

end.
