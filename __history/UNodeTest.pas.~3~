unit UNodeTest;

interface

uses
  DUnitX.TestFramework, UNodes;

type
  [TestFixture]
  TNodeTestObject = class
  strict private
    aStartNode : TStart;
  public
    [Setup]
    procedure Setup;
    [TearDown]
    procedure TearDown;
    [TestCase('Test setze Start-ID','5')]
    procedure setID(newID : Integer);
  end;

implementation

procedure TNodeTestObject.Setup;
begin
  aStartNode := TStart.Create(1);
end;

procedure TNodeTestObject.TearDown;
begin
end;

procedure TNodeTestObject.setID(newID : Integer);
var
  R : Integer;
begin
  R := aStartNode.getID;
  Assert.AreEqual(R,1);
end;


initialization
  TDUnitX.RegisterTestFixture(TNodeTestObject);

end.
